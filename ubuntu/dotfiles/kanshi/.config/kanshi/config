profile home{
  output eDP-1 disable position 0,0
  output DP-4 mode 1920x1080@60.000Hz position 1920,0
  output DP-6 mode 1920x1080@60.000Hz position 0,0
}


profile downtown {
  output eDP-1 disable position 0,0
  output DP-4 mode 2560x1440@60.000Hz position 0,0
  output DP-5 mode 2560x1440@60.000Hz position 2496,0

}

profile unplugged {
  output eDP-1 enable position 0,0
}

