#Office
profile {
  output eDP-1 disable position 0,0
  output DP-4 mode 1920x1080@60.000Hz position 2496,0
  output DP-6 mode 1920x1080@60.000Hz position 4416,0
}



#Work
profile {
  output eDP-1 disable position 0,0
  output DP-4 mode 2560x1440@60.000Hz position 2496,0
  output DP-5 mode 2560x1440@60.000Hz position 5056,0
}

#Unplugged
profile {
  output eDP-1 enable position 0,0
}

